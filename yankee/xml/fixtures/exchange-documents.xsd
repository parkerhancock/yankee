				<element ref="exch:examined-not-printed-without-grant" minOccurs="0"/>
				<element ref="exch:unexamined-printed-without-grant" minOccurs="0"/>
				<element ref="exch:examined-printed-without-grant" minOccurs="0"/>
				<element ref="exch:printed-with-grant" minOccurs="0"/>
				<element ref="exch:claims-only-available" minOccurs="0"/>
				<element ref="exch:not-printed-with-grant" minOccurs="0"/>
				<element name="term-of-grant" type="exch:term-of-grantType" minOccurs="0"/>
				<element ref="exch:invalidation-of-patent" minOccurs="0"/>
				<element ref="exch:printed-as-amended" minOccurs="0"/>
