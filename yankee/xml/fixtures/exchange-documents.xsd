		<choice minOccurs="0" maxOccurs="unbounded">
			<element name="o" type="exch:oType"/>
			<element name="b" type="exch:bType"/>
			<element name="i" type="exch:iType"/>
			<element name="u" type="exch:uType"/>
			<element name="sup" type="exch:supType"/>
			<element name="sub" type="exch:subType"/>
			<element name="smallcaps" type="exch:smallcapsType"/>
			<element name="br" type="exch:brType"/>
			<element name="pre" type="exch:preType"/>
			<element name="dl" type="exch:dlType"/>
			<element name="ul" type="exch:ulType"/>
			<element name="ol" type="exch:olType"/>
